class Transport:
    def __init__(self, speed):
        self.speed = speed

    def move(self):
        print("–á–¥–µ –∑—ñ —à–≤–∏–¥–∫—ñ—Å—Ç—é", self.speed, "–∫–º/–≥–æ–¥")


class Car(Transport):
    pass


class Bicycle(Transport):
    pass


# üîπ –ü—Ä–∏–∫–ª–∞–¥:
car = Car(100)
bike = Bicycle(25)

car.move()
bike.move()
